{
  "expo": {
    "name": "PharmaGO",
    "slug": "PharmaGO",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "entryPoint": "./src/App.tsx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#8FCEFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "sdkVersion": "38.0.0"
  }
}
